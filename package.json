  "description": "The official way of using Azury API 4 Node.JS",
  "main": "src/index.js",
  "homepage": "https://api.azury.cc/",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepack": "npm run build",
